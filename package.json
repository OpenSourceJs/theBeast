{
  "name": "thebeast",
  "version": "1.0.0",
  "engines": {
    "node": "9.2.0"
  },
  "private": true,
  "description":
    "This is a full stack web application react-redux frontend and express and postgres in the backend ",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "prestart": "babel-node bin/scripts/startMessage.js",
    "start": "npm-run-all --parallel lint:watch db  start:client start:server",
    "security-check": "nsp check",
    "test":
      "mocha --watch --reporter progress --require babel-core/register test \"test/**/**/**/*.test.js\"",
    "start:client":
      "webpack -d  --colors --progress --watch webpack.config.babel.js",
    "start:server": "nodemon --watch index --exec babel-node -- index.js",
    "clean": "rm -rf dist && rm -rf node_modules",
    "lint": "esw app",
    "lint:watch": "npm run lint -- --watch",
    "db": "mongod",
    "updates": "ncu -u && npm i",
    "flow": "flow",
    "heroku-prebuild": "bash ./bin/scripts/deployment_react.sh",
    "heroku-postbuild": "bash ./bin/scripts/deployment_cleanup.sh",
    "rebuild": "webpack -d --progress --colors"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OpenSourceJs/theBeast.git"
  },
  "keywords": ["Starter", "kit"],
  "author": "Mohamed Gassama",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/OpenSourceJs/theBeast/issues"
  },
  "homepage": "https://github.com/OpenSourceJs/theBeast#readme",
